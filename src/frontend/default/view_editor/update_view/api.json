{
    "httpMethod": "POST",
    "description": "ビューの設定を変更",
    "commandName": "UPDATE_VIEW",
    "queryParameters": {
        "view": {
            "dataType": "INTEGER",
            "description": "ビューID",
            "isRequired": true,
            "example": 23
        },
        "view_index": {
            "dataType": "INTEGER",
            "description": "ビューのインデックス番号。",
            "isRequired": true,
            "example": 0
        }
    },
    "requestBody": {
        "viewName": {
            "dataType": "TEXT",
            "description": "ビュー名",
            "example": "○○一覧"
        },
        "viewType": {
            "dataType": "TEXT",
            "description": "表示の種類",
            "example": "TABLE"
        },
        "onePageMaxSize": {
            "dataType": "INTEGER",
            "description": "１ページあたりの最大表示数",
            "example": 50
        },
        "excelStartRow": {
            "dataType": "INTEGER",
            "description": "Excelの開始位置（行）",
            "example": 8
        },
        "excelStartColumn": {
            "dataType": "INTEGER",
            "description": "Excelの開始位置（列）",
            "example": 1
        },
        "viewColumns": {
            "title": "ビューカラムの一覧",
            "isArray": true,
            "onePageMaxSize": 35,
            "children": {
                "viewColumnId": {
                    "dataType": "TEXT",
                    "description": "ビューカラムのID",
                    "example": "d4"
                },
                "viewColumnName": {
                    "dataType": "TEXT",
                    "description": "ビューカラムの名前",
                    "example": "氏名"
                }
            }
        }
    },
    "response": {
        "userMessage": {
            "dataType": "TEXT",
            "description": "完了メッセージ",
            "example": "ビュー―の設定を更新しました。"
        },
        "nextUrl": {
            "dataType": "TEXT",
            "description": "完了した場合に、移動すべきURL",
            "example": "./"
        }
    }
}