{
    "httpMethod": "POST",
    "description": "ページ名を変更",
    "commandName": "RENAME_PAGE",
    "queryParameters": {
        "page_id": {
            "dataType": "INTEGER",
            "description": "変更したいページのID",
            "isRequired": true,
            "example": 8
        }
    },
    "requestBody": {
        "pageName": {
            "dataType": "TEXT",
            "description": "新しいページ名",
            "isRequired": true,
            "example": "○○一覧"
        },
        "memo": {
            "dataType": "TEXT",
            "description": "メモ",
            "isRequired": false,
            "example": "○○を入力する際は、○○してください。"
        },
        "isExcel": {
            "dataType": "BOOL",
            "description": "このページをエクセルファイルに変換するか否か",
            "example": true
        }
    },
    "response": {}
}