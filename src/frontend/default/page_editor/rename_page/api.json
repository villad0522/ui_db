{
    "httpMethod": "POST",
    "description": "ページ名を変更",
    "commandName": "RENAME_PAGE",
    "queryParameters": {
        "page_id": {
            "dataType": "NUMBER",
            "description": "変更したいページのID",
            "isRequired": true,
            "example": 8
        }
    },
    "requestBody": {
        "pageName": {
            "dataType": "TEXT",
            "description": "新しいページ名",
            "isRequired": true,
            "example": "○○一覧"
        },
        "memo": {
            "dataType": "TEXT",
            "description": "メモ",
            "isRequired": false,
            "example": "○○を入力する際は、○○してください。"
        }
    },
    "response": {
        "nextUrl": {
            "dataType": "TEXT",
            "description": "完了した場合に、移動すべきURL",
            "example": "./"
        }
    }
}