{
    "httpMethod": "POST",
    "description": "カラムの名前を一括で変更します。",
    "commandName": "UPDATE_COLUMN_NAME",
    "queryParameters": {},
    "requestBody": {
        "tables": {
            "title": "カラム一覧",
            "isArray": true,
            "onePageMaxSize": 35,
            "children": {
                "id": {
                    "dataType": "TEXT",
                    "description": "名前を変更したいカラムのID",
                    "isRequired": true,
                    "example": "c67"
                },
                "name": {
                    "dataType": "TEXT",
                    "description": "変更後のカラム名",
                    "isRequired": true,
                    "example": "氏名"
                }
            }
        }
    },
    "response": {
        "userMessage": {
            "dataType": "TEXT",
            "description": "完了メッセージ",
            "example": "完了しました。"
        },
        "nextUrl": {
            "dataType": "TEXT",
            "description": "完了した場合に、移動すべきURL",
            "example": "../?table=t6"
        }
    }
}