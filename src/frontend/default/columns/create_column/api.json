{
    "httpMethod": "POST",
    "description": "カラムを作成します。",
    "commandName": "CREATE_COLUMN",
    "queryParameters": {
        "table": {
            "dataType": "TEXT",
            "description": "テーブルID",
            "isRequired": true,
            "example": "t3"
        }
    },
    "requestBody": {
        "columnName": {
            "dataType": "TEXT",
            "description": "新しく作成するカラムの名前",
            "isRequired": true,
            "example": "氏名"
        },
        "columnType": {
            "dataType": "TEXT",
            "description": "新しく作成するカラムのデータ型。INTEGER,REAL,TEXT,BOOL,FILE のいずれかを指定するか、リクエストボディ「tables」に対応するインデックス番号を指定してください。",
            "isRequired": true,
            "example": "TEXT"
        },
        "tables": {
            "title": "テーブル一覧",
            "isArray": true,
            "onePageMaxSize": 100,
            "children": {
                "id": {
                    "dataType": "TEXT",
                    "description": "名前を変更したいテーブルのID",
                    "isRequired": true,
                    "example": "t6"
                }
            }
        }
    },
    "response": {
        "columnId": {
            "dataType": "TEXT",
            "description": "新しく作成したカラムのID",
            "example": "c4"
        },
        "userMessage": {
            "dataType": "TEXT",
            "description": "完了メッセージ",
            "example": "再接続しました。"
        },
        "nextUrl": {
            "dataType": "TEXT",
            "description": "完了した場合に、移動すべきURL",
            "example": "../?table=t6"
        }
    }
}