{
    "httpMethod": "POST",
    "description": "レコードを追加します。",
    "commandName": "CREATE_RECORD",
    "queryParameters": {
        "table": {
            "dataType": "TEXT",
            "description": "テーブルID",
            "isRequired": true,
            "example": "t3"
        }
    },
    "requestBody": {
        "columns": {
            "title": "カラム一覧",
            "isArray": true,
            "onePageMaxSize": 35,
            "children": {
                "id": {
                    "dataType": "TEXT",
                    "description": "カラムID",
                    "isRequired": true,
                    "example": "c7"
                },
                "newField": {
                    "dataType": "TEXT",
                    "description": "新しく追加したいフィールド",
                    "isRequired": true,
                    "example": "ああああ"
                }
            }
        }
    },
    "response": {
        "recordId": {
            "dataType": "INTEGER",
            "description": "新しく作成したレコードのID",
            "example": 342
        },
        "userMessage": {
            "dataType": "TEXT",
            "description": "完了メッセージ",
            "example": "再接続しました。"
        },
        "nextUrl": {
            "dataType": "TEXT",
            "description": "完了した場合に、移動すべきURL",
            "example": "./"
        }
    }
}